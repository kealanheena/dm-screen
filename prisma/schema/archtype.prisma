model Archtype {
  id Int @id @default(autoincrement())

  name String
  key  String @unique

  spellcasting_ability Ability_Score?

  player_characters Player_Character[]

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt @default(now())
}

enum Ability_Score {
  STR
  DEX
  CON
  INT
  WIS
  CHA
}
