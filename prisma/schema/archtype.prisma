model Archtype {
  id Int @id @default(autoincrement())

  name String
  key  String @unique

  spellcasting_ability AbilityScore?

  player_characters PlayerCharacter[]

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

enum AbilityScore {
  STR
  DEX
  CON
  INT
  WIS
  CHA
}
